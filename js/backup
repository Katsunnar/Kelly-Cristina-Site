window.onbeforeunload = function() {
    if (window.location.pathname !== "/index.html") {
      window.location.href = "/index.html";
    }
  }

window.onload = function() {
    if (window.location.pathname !== "/index.html" && window.performance.navigation.type === 1) {
      window.location.href = "/index.html";
    }
  }